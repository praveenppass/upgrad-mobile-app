{
	"name": "lexmobileapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android --mode=stagedebug",
		"ios": "react-native run-ios --scheme \"lexmobileappStage\"",
		"lint": "eslint --ext .ts --ext .tsx ./src --fix",
		"start": "react-native start",
		"test": "jest",
		"reset": "rm -rf node_modules/ && rm yarn.lock && watchman watch-del-all && yarn install",
		"pod-install": "cd ios && pod install",
		"clean:ios": "cd ios && rm -rf Pods && rm -rf build && rm -f Podfile.lock && pod install",
		"clean:android": "cd android && ./gradlew clean && rm -rf app/build",
		"prettier-format": "prettier --config .prettierrc.js 'src/**/*.js' --write",
		"postinstall": "npx patch-package",
		"android:stage": "react-native run-android --mode=stagedebug  --appIdSuffix=stage",
		"android:prod": "react-native run-android --mode=proddebug",
		"android:stage-release": "react-native run-android --mode=stagerelease --appIdSuffix=stage",
		"android:prod-release": "react-native run-android --mode=prodrelease",
		"android:stage-debug": "export ENVFILE=.env.stage && cd android && ./gradlew assembleStageDebug && cd ..",
    	"android:prod-debug": "export ENVFILE=.env.prod && cd android && ./gradlew assembleProdDebug && cd ..",
		"ios:stage-release": "react-native run-ios --scheme \"lexMobileAppStage\" --mode=ReleaseStage",
		"ios:stage": "react-native run-ios --scheme \"lexMobileAppStage\" --mode=DebugStage",
		"ios:prod": "react-native run-ios --scheme \"lexMobileApp\" --mode=DebugProd",
		"android:prod.build_aab": "export ENVFILE=.env.prod && cd android && ./gradlew bundleProd && cd ..",
		"code_push-standalone:release-OTA": "sh patches/codepush-standalone-release-ota.sh",
		"code_push-standalone:patch": "sh patches/codepush-standalone-patch.sh",
		"code_push-standalone:history": "sh patches/codepush-standalone-history.sh",
		"figma:setup": "node scripts/figma/setup.js",
		"figma:status": "node scripts/figma/status.js"
	},
